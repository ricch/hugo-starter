html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none}table{border-collapse:collapse;border-spacing:0}body,html{font-family:poppins,sans-serif;color:#333;font-size:16px}img{max-width:100%}a{color:#1a73e8;text-decoration:none}a:hover{text-decoration:underline;text-underline-position:under}main{margin-top:2rem;margin-bottom:2rem}h1{font-size:5rem;margin-bottom:1.5rem}@media(max-width:768px){h1{font-size:2.5rem}}h2{font-size:3rem;margin-bottom:1rem}@media(max-width:768px){h2{font-size:2rem}}h3{font-size:2rem;margin-bottom:.75rem}@media(max-width:768px){h3{font-size:1.5rem}}h4{font-size:1.5rem;margin-bottom:.75rem}@media(max-width:768px){h4{font-size:1.25rem}}h5{font-size:1rem;margin-bottom:.75rem}h1,h2,h3,h4,h5,h6{line-height:1.2}p{font-size:1rem;line-height:1.6;margin-bottom:1rem}header{width:100%;padding-top:2rem;padding-bottom:2rem;background-color:#f8f8f8}header .logo{max-width:150px;flex-grow:1}header .wrapper,header .wrapper-narrow{display:flex;justify-content:space-between;align-items:center}footer{width:100%;padding-top:2rem;padding-bottom:2rem}#navbar ul li{list-style:none;display:inline-block}#navbar ul li a{color:#333;transition:.3s all;padding:.5rem .75rem;margin:0 .5rem}#navbar ul li a:hover{color:#1a73e8}#navbar ul li a.active{color:#1a73e8}#navbar ul li:nth-child(1){margin-left:-1.25rem}#navbar .dropdown{position:relative}#navbar .dropdown::after{content:url(/images/chevron-d.svg);position:absolute;right:-.125rem;top:0;width:10px;height:10px;opacity:.5}#navbar .dropdown:hover .sub-menu{visibility:visible;opacity:1}#navbar .sub-menu{position:absolute;visibility:hidden;opacity:0;top:2rem;width:200px;box-shadow:0 0 .5rem rgba(0,0,0,.1);background-color:rgba(255,255,255,.96);transition:.3s all}#navbar .sub-menu .sub-menu-item{display:block;margin:0;padding:1rem .5rem}#navbar .sub-menu .sub-menu-item a{margin:0;padding:1rem}.wrapper,.wrapper-narrow{max-width:60rem;margin-left:auto;margin-right:auto}@media(max-width:984px){.wrapper,.wrapper-narrow{padding-left:.75rem;padding-right:.75rem}}.wrapper-narrow{max-width:40rem}.flex{display:flex}.flex .flex-1-2{width:calc(100%/2)}.flex .flex-1-3{width:calc(100%/3)}.flex .flex-2-3{width:calc(100%/3*2)}